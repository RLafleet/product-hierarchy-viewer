{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "app-client": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/app-client",
            "sourceRoot": "projects/app-client/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/app-client",
                        "index": "projects/app-client/src/index.html",
                        "main": "projects/app-client/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/app-client/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/app-client/src/favicon.ico",
                            "projects/app-client/src/assets"
                        ],
                        "styles": [
                            "projects/app-client/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "app-client:build:production"
                        },
                        "development": {
                            "browserTarget": "app-client:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "app-client:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/app-client/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/app-client/src/favicon.ico",
                            "projects/app-client/src/assets"
                        ],
                        "styles": [
                            "projects/app-client/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "product-hierarchy-viewer": {
            "projectType": "library",
            "root": "projects/product-hierarchy-viewer",
            "sourceRoot": "projects/product-hierarchy-viewer/src",
            "prefix": "phv",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/product-hierarchy-viewer/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/product-hierarchy-viewer/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/product-hierarchy-viewer/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "tsConfig": "projects/product-hierarchy-viewer/tsconfig.spec.json",
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": "e0bf331d-b436-4601-8b0c-3e1d6559ece5"
    }
}